{

  "name": "choreapp_anna",

  "version": "1.0.0",

  "description": "",

  "main": "./choremain_anna.js",

  "scripts": {

        "start": "electron .",

        "build:win": "electron-builder --win",

        "build:mac": "electron-builder --mac --dir",

        "build": "electron-builder -mw"

    },

    "build": {

        "appId": "com.choreapp_anna",

        "files": [

            "choremain_anna.js",

            "choreindex_anna.html",

            "renderer.js",

            "annaphotos/**/*",

            "100/**/*",

            "*.gif",

            "*.jpg",

            "*.png"

        ],

        "mac": {

            "target": ["dir", "dmg"],

            "category": "public.app-category.utilities"

        },

        "win": {

            "target": "portable"

        }

    },



  "keywords": [],

  "author": "",

  "license": "ISC",

  "devDependencies": {

      "electron": "^28.3.3",

      "electron-builder": "^24.9.1"

  },

  "dependencies": {

      "@electron/remote": "^2.1.2"

  }

}